{
  "name": "typescript-restful-starter",
  "version": "0.1.0",
  "description": "kit starter node application",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && cd dist && cross-env NODE_ENV=PRODUCTION node -r dotenv/config Index.js",
    "dev": "nodemon",
    "test": "cross-env NODE_ENV=test mocha test/**/*.ts",
    "build": "npm run clean && tsc --outDir dist && cp .env ./dist",
    "clean": "rimraf dist"
  },
  "keywords": [
    "node",
    "express",
    "typeorm",
    "typescript",
    "tslint",
    "JWT",
    "E2015"
  ],
  "author": "Hector Riquelme",
  "license": "MIT",
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.11.1",
    "@types/joi": "^13.6.1",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/method-override": "0.0.31",
    "@types/node": "^10.5.2",
    "body-parser": "^1.18.2",
    "camesine": "^1.0.1",
    "class-validator": "^0.9.1",
    "cluster": "^0.7.7",
    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "joi": "^14.0.0",
    "jsonwebtoken": "^8.2.0",
    "method-override": "^3.0.0",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "superagent": "^4.0.0-beta.5",
    "token-extractor": "^0.1.6",
    "ts-node": "^7.0.0",
    "typeorm": "^0.3.17",
    "typescript": "^3.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/cors": "^2.8.4",
    "@types/express-jwt": "0.0.40",
    "@types/mocha": "^5.2.4",
    "@types/morgan": "^1.7.35",
    "@types/rimraf": "2.0.2",
    "@types/superagent": "^3.5.7",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "mocha": "^5.0.4",
    "nodemon": "^1.17.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "tslint": "^5.9.1"
  }
}
